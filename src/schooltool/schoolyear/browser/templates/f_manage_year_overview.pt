<div class="manage-view" i18n:domain="schooltool"
     tal:condition="view/has_schoolyear"
     tal:define="year view/schoolyear">
  <h2>
    <a title="Manage year"
       tal:attributes="href year/@@absolute_url"
       i18n:attributes="title">
      <tal:block content="year/first/@@mediumDate" /> -
      <tal:block content="year/last/@@mediumDate" />
    </a>
  </h2>
</div>
